        <!-- Rest of the header content -->
        <div class="absolute inset-0 bg-gradient-to-b from-black/60 to-black/30 z-10"></div>
        <img src="https://images.unsplash.com/photo-1464822759023-fed622ff2c3b?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1440&h=600&q=80"
            class="absolute inset-0 w-full h-full object-cover" alt="Hero background" />
        <div class="relative z-20 max-w-8xl mx-auto px-6 pt-32 hero-text">
            <h1 class="text-6xl font-bold text-white mb-6">Your Journey, <br />Our Passion</h1>
            <p class="text-xl text-white/90 mb-8 max-w-2xl">Explore the world with 5starjourneyâ€”where every trip is
                tailored, every destination unforgettable, and every moment filled with wonder.</p>
            <a href="https://wa.me/918238773780" class="!rounded-button bg-custom text-white px-8 py-3 text-lg">Book
                Now</a>
        </div>
    </header>
    <main class="max-w-8xl mx-auto px-6 py-16">
        <!-- Tour image carousel and description -->
        <div class="max-w-8xl mx-auto px-6 py-16">
            <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
                <div class="md:col-span-2">
                    <div class="relative w-full h-96 mb-8">
                        <div class="carousel relative overflow-hidden rounded-xl">
                            <div class="flex transition-transform duration-500">
                                <% if (tour.images && tour.images.length> 0) { %>
                                    <% tour.images.forEach((image, index)=> { %>
                                        <img src="<%= image %>" class="w-full h-96 object-cover flex-shrink-0"
                                            alt="<%= tour.title %> View <%= index + 1 %>" />
                                        <% }); %>
                                            <% } else { %>
                                                <img src="/assets/images/tours/default-tour.jpg"
                                                    class="w-full h-96 object-cover flex-shrink-0"
                                                    alt="Default Tour Image" />
                                                <% } %>
                            </div>
                            <button
                                class="absolute left-4 top-1/2 -translate-y-1/2 bg-white/80 hover:bg-white text-gray-800 p-2 rounded-full shadow-lg">
                                <i class="fas fa-chevron-left"></i>
                            </button>
                            <button
                                class="absolute right-4 top-1/2 -translate-y-1/2 bg-white/80 hover:bg-white text-gray-800 p-2 rounded-full shadow-lg">
                                <i class="fas fa-chevron-right"></i>
                            </button>
                            <div class="absolute bottom-4 left-1/2 -translate-x-1/2 flex space-x-2">
                                <% if (tour.images && tour.images.length> 0) { %>
                                    <% tour.images.forEach((_, index)=> { %>
                                        <button
                                            class="w-2 h-2 rounded-full bg-white/60 hover:bg-white <%= index === 0 ? 'bg-white' : '' %>"></button>
                                        <% }); %>
                                            <% } else { %>
                                                <button class="w-2 h-2 rounded-full bg-white"></button>
                                                <% } %>
                            </div>
                        </div>
                    </div>
                    <h1 class="text-4xl font-bold mb-4 dark:text-white">
                        <%= tour.title %>
                    </h1>
                    <div class="flex items-center space-x-4 mb-6">
                        <span class="bg-custom/10 text-custom px-3 py-1 rounded-full">
                            <%= tour.duration %>
                        </span>
                        <span class="bg-custom/10 text-custom px-3 py-1 rounded-full">
                            <%= tour.category %>
                        </span>
                        <span class="bg-custom/10 text-custom px-3 py-1 rounded-full">
                            <%= tour.difficulty %>
                        </span>
                    </div>
                    <div class="prose dark:prose-invert max-w-none mb-8">
                        <h2 class="text-2xl font-semibold mb-4">Tour Overview</h2>
                        <p class="text-gray-600 dark:text-gray-400">
                            <%= tour.description %>
                        </p>

                        <!-- 2. Highlights -->
                        <% if (tour.highlights && tour.highlights.length> 0) { %>
                            <h2 class="text-2xl font-semibold mb-4 mt-8">Highlights</h2>
                            <ul class="list-disc pl-6 text-gray-600 dark:text-gray-400">
                                <% tour.highlights.forEach(highlight=> { %>
                                    <li>
                                        <%= highlight %>
                                    </li>
                                    <% }); %>
                            </ul>
                            <% } %>

                                <!-- 3. Itinerary -->
                                <% if (tour.itinerary) { %>
                                    <h2 class="text-2xl font-semibold mb-4 mt-8">Itinerary</h2>
                                    <div class="space-y-4">
                                        <% Object.keys(tour.itinerary).forEach(day=> { %>
                                            <div class="border-l-4 border-custom pl-4">
                                                <h3 class="font-semibold">
                                                    <%= tour.itinerary[day].title || `Day ${day}` %>
                                                </h3>
                                                <p class="text-gray-600 dark:text-gray-400">
                                                    <%= tour.itinerary[day].description %>
                                                </p>
                                            </div>
                                            <% }); %>
                                    </div>
                                    <% } %>
                    </div>
                </div>
                <div class="bg-white dark:bg-gray-800 p-6 rounded-xl shadow-lg h-fit sticky top-8">
                    <h3 class="text-2xl font-semibold mb-4 dark:text-white">Tour Details</h3>
                    <div class="space-y-4 mb-6">
                        <div class="flex justify-between">
                            <span class="text-gray-600 dark:text-gray-400">Duration:</span>
                            <span class="font-semibold dark:text-white">
                                <%= tour.duration %>
                            </span>
                        </div>
                        <div class="flex justify-between">
                            <span class="text-gray-600 dark:text-gray-400">Group Size:</span>
                            <span class="font-semibold dark:text-white">
                                <%= tour.groupSize || 'Not specified' %> max
                            </span>
                        </div>
                        <div class="flex justify-between">
                            <span class="text-gray-600 dark:text-gray-400">Difficulty:</span>
                            <span class="font-semibold dark:text-white">
                                <%= tour.difficulty %>
                            </span>
                        </div>
                        <div class="flex justify-between">
                            <span class="text-gray-600 dark:text-gray-400">Starting Point:</span>
                            <span class="font-semibold dark:text-white">
                                <%= tour.startingPoint || 'Not specified' %>
                            </span>
                        </div>
                        <% if (tour.price) { %>
                            <div class="flex justify-between">
                                <span class="text-gray-600 dark:text-gray-400">Price:</span>
                                <span class="font-semibold dark:text-white">â‚¹<%= tour.price.toLocaleString() %></span>
                            </div>
                            <% } %>
                    </div>
                    <a href="https://wa.me/918238773780"
                        class="!rounded-button bg-custom text-white px-6 py-3 w-full mb-4 block text-center">Book
                        Now</a>
                    <% if (tour.downloadLink) { %>
                        <a href="<%= tour.downloadLink %>" target="_blank"
                            class="!rounded-button border-2 border-custom text-custom bg-transparent px-6 py-3 w-full block text-center">Download
                            Brochure</a>
                        <% } else { %>
                            <button
                                class="!rounded-button border-2 border-custom text-custom bg-transparent px-6 py-3 w-full"
                                disabled>Brochure Coming Soon</button>
                            <% } %>
                </div>
            </div>
        </div>

        <!-- 4. Popular Attractions -->
        <% if (tour.attractions && tour.attractions.length> 0) { %>
            <section class="py-16">
                <div class="max-w-8xl mx-auto px-6">
                    <h2 class="text-4xl font-bold text-center mb-12 dark:text-white">Popular Attractions</h2>
                    <div class="grid grid-cols-1 md:grid-cols-4 gap-8">
                        <% tour.attractions.forEach(attraction=> { %>
                            <div class="group rounded-xl overflow-hidden shadow-lg h-[400px]">
                                <div class="relative h-64 overflow-hidden">
                                    <img src="<%= attraction.imageUrl %>"
                                        class="w-full h-full object-cover transform group-hover:scale-110 transition-transform duration-500"
                                        alt="<%= attraction.name %>" />
                                </div>
                                <div class="p-6 bg-white dark:bg-gray-800">
                                    <h3 class="text-xl font-semibold mb-2 dark:text-white">
                                        <%= attraction.name %>
                                    </h3>
                                    <p class="text-gray-600 dark:text-gray-400 mb-4 line-clamp-3">
                                        <%= attraction.description %>
                                    </p>
                                    <a href="#" class="text-custom hover:text-custom/80">Learn more â†’</a>
                                </div>
                            </div>
                            <% }); %>
                    </div>
                </div>
            </section>
            <% } %>

                <!-- 5. Frequently Asked Questions -->
                <% if (faqs && faqs.length> 0) { %>
                    <section class="py-16 bg-gray-50 dark:bg-gray-800 rounded-2xl">
                        <div class="max-w-8xl mx-auto px-6">
                            <h2 class="text-4xl font-bold text-center mb-12 dark:text-white">Frequently Asked Questions
                            </h2>
                            <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
                                <% faqs.forEach(faq=> { %>
                                    <div
                                        class="faq-item bg-white dark:bg-gray-700 rounded-xl p-6 cursor-pointer transition-all hover:shadow-lg">
                                        <div class="flex justify-between items-center">
                                            <h3 class="text-lg font-semibold dark:text-white">
                                                <%= faq.question %>
                                            </h3>
                                            <i class="fas fa-chevron-down text-custom transition-transform"></i>
                                        </div>
                                        <div class="faq-answer mt-4 text-gray-600 dark:text-gray-300 hidden overflow-hidden transition-all duration-300"
                                            style="max-height: 0;">
                                            <%= faq.answer %>
                                        </div>
                                    </div>
                                    <% }); %>
                            </div>
                        </div>
                    </section>
                    <% } %>
    </main>
