    <title>
        <%= title %> | 5starjourney
    </title>

    '

    ' <!-- Favicon -->
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/images/favicon-16x16.png">
    <link rel="manifest" href="/assets/images/site.webmanifest">
    <!-- Fonts and styles -->
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&amp;display=swap"
        rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet" />
    <link href="/css/styles.css" rel="stylesheet" />
    <script
        src="https://cdn.tailwindcss.com/3.4.5?plugins=forms@0.5.7,typography@0.5.13,aspect-ratio@0.4.2,container-queries@0.1.1"></script>
    <script src="https://ai-public.creatie.ai/gen_page/tailwind-config.min.js" data-color="#EB1F55"
        data-border-radius="medium"></script>
    <style>
        /* Add this to your existing styles */
        html {
            scroll-behavior: smooth;
        }

        section {
            scroll-margin-top: 70px;
        }

        /* Mobile menu styles */
        #mobile-menu {
            position: fixed;
            top: 0;
            right: -300px;
            /* Start off-screen */
            width: 300px;
            height: 100vh;
            background-color: #0b1019;
            backdrop-filter: blur(10px);
            z-index: 100;
            transition: right 0.3s ease-in-out;
            border-left: 1px solid rgba(255, 255, 255, 0.1);
            overflow-y: auto;
        }

        #mobile-menu.active {
            right: 0;
        }

        .mobile-menu-close {
            position: absolute;
            top: 20px;
            right: 20px;
            color: white;
            background: none;
            border: none;
            font-size: 24px;
            cursor: pointer;
        }

        .mobile-menu-content {
            padding-top: 60px;
            display: flex;
            flex-direction: column;
            padding-left: 30px;
            padding-right: 30px;
        }

        .mobile-menu-content a {
            padding: 15px 0;
            font-size: 18px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.1);
        }

        .mobile-menu-overlay {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: 99;
            display: none;
        }

        .mobile-menu-overlay.active {
            display: block;
        }

        /* Responsive adjustments */
        @media (max-width: 768px) {
            .hero-text h1 {
                font-size: 2.5rem;
            }

            .hero-text p {
                font-size: 1rem;
            }
        }
    </style>
</head>

<body class="font-sans dark:bg-gray-900">
    <header class="relative h-[600px]">
        <nav class="sticky top-6 left-0 right-0 z-50">
            <div class="mx-4">
                <div class="backdrop-blur-md bg-black/20 border border-white/10 rounded-2xl shadow-lg">
                    <div class="flex items-center justify-between container mx-auto px-6 py-4">

                        <a href="/" class="text-3xl font-bold text-white tracking-wider">
                            <span class="text-[#ff0066]">5</span>STARJOURNEY
                        </a>
                        <div class="hidden md:flex items-center space-x-8">
                            <a href="/"
                                class="text-white hover:text-[#ff0066] transition-colors duration-300 text-sm">HOME</a>
                            <a href="/#Services"
                                class="text-white hover:text-[#ff0066] transition-colors duration-300 text-sm">SERVICES</a>
                            <a href="/#Domestic"
                                class="text-white hover:text-[#ff0066] transition-colors duration-300 text-sm">DOMESTIC</a>
                            <a href="/#International"
                                class="text-white hover:text-[#ff0066] transition-colors duration-300 text-sm">INTERNATIONAL</a>
                            <a href="#Footer"
                                class="text-white hover:text-[#ff0066] transition-colors duration-300 text-sm">CONTACT</a>
                            <a href="/admin/login"
                                class="bg-[#ff0066] text-white px-6 py-2 rounded-full text-sm font-medium hover:bg-[#ff0066]/90 transition-all duration-300">Admin</a>
                        </div>
                        <button id="mobile-menu-button"
                            class="md:hidden text-white hover:text-[#ff0066] transition-colors duration-300">
                            <i class="fas fa-bars text-2xl"></i>
                        </button>
                    </div>
                </div>
            </div>
        </nav>

        <!-- Mobile menu overlay -->
        <div id="mobile-menu-overlay" class="mobile-menu-overlay"></div>

        <!-- Mobile menu -->
        <div id="mobile-menu">
            <button id="mobile-menu-close" class="mobile-menu-close">
                <i class="fas fa-times"></i>
            </button>
            <div class="mobile-menu-content">
